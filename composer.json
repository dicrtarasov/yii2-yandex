{
    "name": "dicr/yii2-yandex",
    "type": "yii2-extension",
    "description": "Яндекс API для Yii2",
    "homepage": "https://gitlab.com/dicr/yii2-yandex",
    "authors": [
        {
            "name": "Igor Tarasov",
            "email": "develop@dicr.org",
            "homepage": "http://dicr.org",
            "role": "developer"
        }
    ],
    "keywords": [
        "dicr yii2 yandex"
    ],
    "support": {
        "source": "https://gitlab.com/dicr/yii2-yandex.git"
    },
    "require": {
        "php": ">=7.2",
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-httpclient": "~2.0",
        "dicr/yii2-http": "~1.0",
        "yandex-money/yandex-checkout-sdk-php": "1.5.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }, {
            "type": "git",
            "url": "https://gitlab.com/dicr/yii2-http.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "dicr\\yandex\\": "src/"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "~8.1"
    },
    "license": "Apache-2.0"
}
